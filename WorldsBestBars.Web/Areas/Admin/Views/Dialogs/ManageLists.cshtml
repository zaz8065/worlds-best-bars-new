<div class="action-dialog manage-lists">
  <div class="left ui-widget ui-widget-content ui-corner-left">
    <div class="scrollable" style="overflow: auto; width: 250px; height: 553px">
      <div class="list">
      </div>
      <button class="create">
        Create New List</button>
    </div>
  </div>
  <div class="right ui-corner-right">
    <ul>
      <li class="tab-head"><a href="#manage-lists-right-search">Contents</a></li>
      <li class="tab-head"><a href="#manage-lists-right-info">Information</a></li>
      <li class="tab-head"><a href="#manage-lists-right-related">Related</a></li>
    </ul>
    <div class="manage-lists-right-search">
      <div class="related">
        <div class="existing">
          <label>
            Existing (click to remove)</label>
          <div class="search-results">
          </div>
        </div>
        <div class="add">
          <div class="search">
            <label for="search">
              Search (click to add)</label>
            <input type="text" id="search" name="search" class="text ui-widget-content ui-corner-all" />
          </div>
          <div class="search-results">
          </div>
        </div>
      </div>
    </div>
    <div class="manage-lists-right-info">
      <form>
      <label for="name">
        Name</label>
      <input type="text" name="name" id="name" class="text ui-widget-content ui-corner-all" />
      <label for="key">
        Key</label>
      <input type="text" name="key" id="key" class="text ui-widget-content ui-corner-all" />
      <label for="group">
        Group</label>
      <input type="text" name="group" id="group" class="text ui-widget-content ui-corner-all" />
      <label for="searchable">
        Is Searchable</label>
      <input type="checkbox" name="searchable" id="searchable" />
      <button class="save">Save</button>
      </form>
    </div>
    <div class="manage-lists-right-related">
      <div class="related-2">
        <div class="existing">
          <label>
            Existing (click to remove)</label>
          <div class="search-results">
          </div>
        </div>
        <div class="add">
          <div class="search">
            <label for="search-related">
              Search (click to add)</label>
            <input type="text" id="search-related" name="search" class="text ui-widget-content ui-corner-all" />
          </div>
          <div class="search-results">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
